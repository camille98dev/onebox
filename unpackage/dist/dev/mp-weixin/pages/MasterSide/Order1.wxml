<view class="data-v-8c912220"><block wx:for="{{items}}" wx:for-item="item" wx:for-index="__i0__" wx:key="orderNumber"><view class="orderContainer data-v-8c912220"><view class="orderAndStatusContainer data-v-8c912220"><view class="orderNumberContainer data-v-8c912220"><text class="orderText data-v-8c912220">订单号：</text><text class="orderNumber data-v-8c912220">{{item.orderNumber}}</text></view><view class="orderStatus data-v-8c912220"><text class="status data-v-8c912220">{{item.status}}</text></view></view><view style="display:flex;margin-top:30rpx;" class="data-v-8c912220"><view style="margin-right:15rpx;" class="data-v-8c912220"><image class="imageSize data-v-8c912220" src="../../static/productImage.png"></image></view><view class="productInformation data-v-8c912220"><view class="productDescriptionContainer data-v-8c912220"><text class="description data-v-8c912220">{{item.description}}</text></view><view class="numberofBoxes data-v-8c912220" style="align-items:center;"><text class="boxes data-v-8c912220">{{item.box}}</text><text class="additional data-v-8c912220">{{item.addtion}}</text></view></view></view><view class="moreDetailsContainer data-v-8c912220"><view class="timeServiceContainer data-v-8c912220"><view class="serviceContainer data-v-8c912220"><text class="serviceTimeText data-v-8c912220">服务时间：</text><text class="exactDate data-v-8c912220">{{item.date}}</text></view><view class="timeLimitContainer data-v-8c912220"><text class="limitText data-v-8c912220">规定时间：</text><text class="timelimit data-v-8c912220">{{item.limit}}</text></view></view><view class="customeradress data-v-8c912220"><text class="gui-form-label data-v-8c912220">客户地址：</text><view class="gui-form-body data-v-8c912220"><picker mode="selector" range="{{adress}}" data-event-opts="{{[['change',[['pickerChange',['$event']]]]]}}" bindchange="__e" class="data-v-8c912220"><view class="gui-flex gui-rows gui-space-between gui-align-items-center data-v-8c912220"><text class="gui-text data-v-8c912220">{{adress[adressIndex]}}</text><image class="pinSize data-v-8c912220" src="../../static/PinLocation.png"></image></view></picker></view></view><view class="baseWage data-v-8c912220"><text class="baseWageText data-v-8c912220">基础工价</text><text class="baseWageAmount data-v-8c912220">{{item.WageAmount}}</text></view><view class="gradeAllowance data-v-8c912220"><text class="gradeAllowanceText data-v-8c912220">等级补贴</text><text class="gradeAllowanceAmount data-v-8c912220">{{item.gradeAllowance}}</text></view><view class="policySubsidy data-v-8c912220"><text class="subsidyText data-v-8c912220">政策补贴</text><text class="subsidyAmount data-v-8c912220">{{item.subsidyAmount}}</text></view><view class="totalContainer data-v-8c912220"><text class="totalText data-v-8c912220">总计</text><text class="totalAmount data-v-8c912220">{{item.totalAmount}}</text></view></view><block wx:if="{{item.status==='进行中'}}"><view class="optionsButtonContainer data-v-8c912220"><view class="data-v-8c912220"><button class="contact data-v-8c912220">联系客户</button></view><view class="data-v-8c912220"><button class="service data-v-8c912220">完成服务</button></view></view></block><block wx:if="{{item.status==='待审核'}}"><view data-event-opts="{{[['tap',[['toReview',['$0'],[[['items','orderNumber',item.orderNumber,'status']]]]]]]}}" class="productStatusContainer data-v-8c912220" bindtap="__e"><text class="statusUpdate data-v-8c912220">订单正在审核中，请耐心等候</text></view></block><block wx:if="{{item.status==='完全的'}}"><view class="productStatusContainer data-v-8c912220"><text class="statusUpdate data-v-8c912220">客户暂未评价</text></view></block><view class="reviewContainer data-v-8c912220"><block wx:if="{{item.status==='已完成'}}"><view class="data-v-8c912220"><button data-event-opts="{{[['tap',[['toReview',['$0'],[[['items','orderNumber',item.orderNumber,'status']]]]]]]}}" class="reviews data-v-8c912220" bindtap="__e">查看评价</button></view></block></view></view></block></view>